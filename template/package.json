{
  "name": "create-workers",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "env": "dotenv -e .env -e .env.local --",
    "postinstall": "patch-package",
    "preb": "npm --no-git-tag-version version minor",
    "b": "npm run push",
    "commit": "git add . && git commit -am ",
    "c": "npm run commit",
    "g": "npm run commit",
    "postg": "npm run postpush",
    "push": "npm run commit",
    "_push": "git push origin",
    "_pull": "git pull origin",
    "postpush": "npm run _push",
    "pull": "npm run _pull",
    "build-routes": "mkdir -p dist; mkdir -p dist/client; node scripts/build-routes.mjs",
    "prebuild": "pnpm run build-routes",
    "predev": "pnpm run build-routes",
    "dev": "dotenv -- wrangler dev",
    "build": "dotenv -- vite build",
    "preview": "npm run build && wrangler dev",
    "predeploy": "mkdir -p dist",
    "deploy": "wrangler deploy",
    "reset": "rm -rf node_modules; rm package-lock.json; rm pnpm-lock.yaml; pnpm i"
  },
  "dependencies": {
    "@vueuse/core": "^9.6.0",
    "@vueuse/integrations": "^9.11.0",
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^4.2.5",
    "axios": "^1.2.1",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "nanoid": "4.0.2",
    "universal-cookie": "^7.2.0",
    "vue": "3.5.13",
    "vue-router": "^4.3.0",
    "xodel": "latest"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "latest",
    "@primevue/auto-import-resolver": "latest",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "latest",
    "@vitejs/plugin-vue-jsx": "latest",
    "@vue/eslint-config-prettier": "latest",
    "@vue/eslint-config-typescript": "latest",
    "@vue/test-utils": "latest",
    "@vue/tsconfig": "latest",
    "antdv-component-resolver": "^1.0.7",
    "dotenv-cli": "^6.0.0",
    "eslint": "latest",
    "eslint-plugin-vue": "latest",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "patch-package": "^7.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.10.2",
    "vite": "latest",
    "vite-plugin-node-polyfills": "^0.15.0",
    "vitest": "latest",
    "vue-tsc": "latest",
    "wrangler": "latest"
  }
}