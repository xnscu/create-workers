{
  "name": "create-workers",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "env": "dotenv -e .env -e .env.local --",
    "c": "git add . && git commit -am ",
    "g": "yarn c",
    "postg": "git push origin master",
    "pull": "git pull origin master ",
    "preb": "npm --no-git-tag-version version minor",
    "b": "yarn build && yarn g",
    "build-routes": "mkdir -p dist; mkdir -p dist/client; node scripts/build-routes.mjs",
    "prebuild": "yarn build-routes",
    "build": "yarn env vite build",
    "predeploy": "mkdir -p dist",
    "deploy": "yarn build && wrangler deploy",
    "predev": "yarn build-routes",
    "dev": "yarn env vite",
    "format": "prettier --write . --config .prettierrc.json",
    "postinstall": "patch-package",
    "lint": "yarn format; eslint . --fix",
    "preview": "yarn build && wrangler dev",
    "reset": "rm -rf node_modules; rm package-lock.json; rm pnpm-lock.yaml; pnpm i"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.5",
    "@vueuse/core": "^9.6.0",
    "@vueuse/integrations": "^9.11.0",
    "axios": "^1.2.1",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.0",
    "universal-cookie": "^7.2.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "xodel": "latest"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "latest",
    "@primevue/auto-import-resolver": "latest",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "latest",
    "@vue/tsconfig": "^0.8.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "~10.4.0",
    "jiti": "^2.5.1",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^8.0.4",
    "patch-package": "^7.0.0",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.10.2",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-node-polyfills": "^0.15.0",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "latest",
    "vue-tsc": "^3.1.0",
    "wrangler": "latest"
  }
}
