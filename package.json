{
  "name": "create-workers",
  "version": "0.8.0",
  "description": "Start an opinioned cloudflare workers + pages project with vue 3 + vite.",
  "type": "module",
  "bin": {
    "create": "outfile.cjs"
  },
  "files": [
    ".env",
    "locales",
    "outfile.cjs",
    "template"
  ],
  "scripts": {
    "preb": "npm --no-git-tag-version version minor",
    "b": "yarn push",
    "commit": "git add . && git commit -am ",
    "c": "yarn commit",
    "g": "yarn push",
    "push": "yarn commit",
    "_push": "git push origin",
    "_pull": "git pull origin",
    "postpush": "uname -s | grep -q Darwin && yarn _push || while true; do timeout 10 yarn _push && break; done",
    "pull": "uname -s | grep -q Darwin && yarn _pull || while true; do timeout 10 yarn _pull && break; done",
    "lab": "pnpm build && ./outfile.cjs",
    "format": "prettier --write . --config .prettierrc.json",
    "sync": "./scripts/sync.sh download",
    "build": "zx ./scripts/build.mjs",
    "snapshot": "zx ./scripts/snapshot.mjs",
    "pretest": "run-s build snapshot",
    "test": "zx ./scripts/test.mjs",
    "reset": "rm -rf node_modules; rm package-lock.json; rm pnpm-lock.yaml; pnpm i",
    "prepublishOnly": "pnpm build",
    "prepublishOnly2": "zx ./scripts/prepublish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xnscu/create-workers.git"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "vue",
    "vite",
    "primevue",
    "vueuse"
  ],
  "author": "Nan Xiang <xnscu@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xnscu/create-workers/issues"
  },
  "homepage": "https://github.com/xnscu/create-workers#readme",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.16",
    "@types/prompts": "^2.4.9",
    "@vue/create-eslint-config": "^0.3.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.5.1",
    "ejs": "^3.1.9",
    "esbuild": "^0.18.20",
    "esbuild-plugin-license": "^1.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "~10.4.0",
    "gradient-string": "^2.0.2",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.1",
    "minimist": "^1.2.8",
    "npm-run-all2": "^6.1.2",
    "prettier": "3.6.2",
    "prompts": "^2.4.2",
    "zx": "^7.2.3"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.5",
    "primevue": "^4.4.0"
  }
}
